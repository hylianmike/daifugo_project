<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.daifugo.EndController">
   <children>
      <BorderPane prefHeight="800.0" prefWidth="1000.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <bottom>
            <Button fx:id="playAgainButton" mnemonicParsing="false" onAction="#playAgain" text="Play Again" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="50.0" />
               </BorderPane.margin>
            </Button>
         </bottom>
         <center>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Label text="Rankings:">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                     <font>
                        <Font name="System Bold" size="28.0" />
                     </font></Label>
                   <Label fx:id="rankOneLabel" text="#1: Player One">
                       <VBox.margin>
                           <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                       </VBox.margin>
                       <font>
                           <Font size="28.0" />
                       </font></Label>
                   <Label fx:id="rankTwoLabel" text="#2: Player Three">
                       <VBox.margin>
                           <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                       </VBox.margin>
                       <font>
                           <Font size="28.0" />
                       </font></Label>
                   <Label fx:id="rankThreeLabel" text="#3: Player Four">
                       <VBox.margin>
                           <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                       </VBox.margin>
                       <font>
                           <Font size="28.0" />
                       </font></Label>
                   <Label fx:id="rankFourLabel" text="#4: Player Two">
                       <VBox.margin>
                           <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                       </VBox.margin>
                       <font>
                           <Font size="28.0" />
                       </font></Label>
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
